{
  "name": "vue-tel-num-input",
  "version": "0.0.3",
  "type": "module",
  "author": {
    "name": "Mark Minerov",
    "email": "markminerov123@gmail.com",
    "url": "https://github.com/MarkMinerov"
  },
  "description": "A flexible and fully customizable Vue 3 component for phone number input with country code selection, flags, masking, and localization.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkMinerov/vue3-tel-input.git"
  },
  "keywords": [
    "vue3",
    "vue",
    "vue-component",
    "phone-input",
    "telephone-input",
    "tel-input",
    "vue3-component",
    "vue3-phone-input",
    "vue3-tel-input",
    "vue3-telephone-input",
    "vue3-telnum-input",
    "vue3-tel-num-input"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "style": "dist/css/style.css",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./style.css": "./dist/css/style.css",
    "./flags.css": "./dist/css/sprite.css"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "watch:build": "vite build --watch",
    "build": "npm-run-all clean build:once",
    "build:once": "vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "libphonenumber-js": "^1.12.15",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.92.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.2.1",
    "vitepress": "^2.0.0-alpha.12",
    "vue-tsc": "^3.0.5"
  },
  "dependencies": {
    "country-flag-emoji-polyfill": "^0.1.8"
  }
}